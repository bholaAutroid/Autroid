                        android:layout_height="wrap_content">


                        <RelativeLayout
                            android:background="@drawable/bg_aws_items"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dimen_70dp">


                            <ImageView
                                android:layout_marginLeft="@dimen/dimen_10dp"
