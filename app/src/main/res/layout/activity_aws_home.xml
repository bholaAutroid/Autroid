                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="25,53,000"
                        android:textColor="@color/white_color"
                        android:textSize="35sp" />

                </RelativeLayout>
            </RelativeLayout>

            <LinearLayout
