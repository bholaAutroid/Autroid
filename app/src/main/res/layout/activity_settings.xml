<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:background="@color/black"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".aws.navigation.adapter.SettingsActivity">

    <include
        android:elevation="@dimen/dimen_10dp"
        android:id="@id/common_toolbar"
        layout="@layout/setting_tool"
        android:layout_width="match_parent"
        android:layout_height="50dp"/>



    <LinearLayout
        android:layout_marginTop="@dimen/dimen_50dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">



        <RelativeLayout
            android:layout_marginRight="@dimen/dimen_10dp"
            android:layout_marginLeft="@dimen/dimen_10dp"
            android:padding="@dimen/dimen_10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_genral"
                android:layout_gravity="center_vertical"
                android:layout_margin="@dimen/dimen_5dp"
                android:background="@drawable/ic_general_maintenance"
                android:layout_width="@dimen/dimen_30dp"
                android:layout_height="@dimen/dimen_30dp" />

            <TextView
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/iv_genral"
                android:id="@+id/tv_genral"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/dimen_10dp"
                android:textStyle="bold"
                style="@style/style_rotobo_normal"
                android:textColor="@color/gray_color"
                android:textSize="@dimen/dimen_21dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="General" />


            <ImageView
                android:layout_alignParentEnd="true"
                android:layout_margin="@dimen/dimen_5dp"
                android:backgroundTint="@color/white_color"
                android:background="@drawable/ic_baseline_chevron_right_24"
                android:layout_width="@dimen/dimen_30dp"
                android:layout_height="@dimen/dimen_30dp" />

        </RelativeLayout>
        <View
            android:background="@color/gray_color"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1dp"/>


        <RelativeLayout
            android:layout_marginRight="@dimen/dimen_10dp"
            android:layout_marginLeft="@dimen/dimen_10dp"
            android:padding="@dimen/dimen_10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_salesandpurchase"
                android:layout_gravity="center_vertical"
                android:layout_margin="@dimen/dimen_5dp"
                android:background="@drawable/ic_general_maintenance"
                android:layout_width="@dimen/dimen_30dp"
                android:layout_height="@dimen/dimen_30dp" />

            <TextView
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/iv_salesandpurchase"
                android:id="@+id/tv_salesandPurchase"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/dimen_10dp"
                android:textStyle="bold"
                style="@style/style_rotobo_normal"
                android:textColor="@color/gray_color"
                android:textSize="@dimen/dimen_21dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sales and Purchase" />


            <ImageView
                android:layout_alignParentEnd="true"
                android:layout_margin="@dimen/dimen_5dp"
                android:backgroundTint="@color/white_color"
                android:background="@drawable/ic_baseline_chevron_right_24"
                android:layout_width="@dimen/dimen_30dp"
                android:layout_height="@dimen/dimen_30dp" />

        </RelativeLayout>
        <View
            android:background="@color/gray_color"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1dp"/>



        <RelativeLayout
            android:layout_marginRight="@dimen/dimen_10dp"
            android:layout_marginLeft="@dimen/dimen_10dp"
            android:padding="@dimen/dimen_10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_tax"
                android:layout_gravity="center_vertical"
                android:layout_margin="@dimen/dimen_5dp"
                android:background="@drawable/ic_general_maintenance"
                android:layout_width="@dimen/dimen_30dp"
                android:layout_height="@dimen/dimen_30dp" />

            <TextView
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/iv_tax"
                android:id="@+id/tv_tax"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/dimen_10dp"
                android:textStyle="bold"
                style="@style/style_rotobo_normal"
                android:textColor="@color/gray_color"
                android:textSize="@dimen/dimen_21dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tax and Accounts" />


            <ImageView
                android:layout_alignParentEnd="true"
                android:layout_margin="@dimen/dimen_5dp"
                android:backgroundTint="@color/white_color"
                android:background="@drawable/ic_baseline_chevron_right_24"
                android:layout_width="@dimen/dimen_30dp"
                android:layout_height="@dimen/dimen_30dp" />

        </RelativeLayout>
        <View
            android:background="@color/gray_color"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1dp"/>


        <RelativeLayout
            android:layout_marginRight="@dimen/dimen_10dp"
            android:layout_marginLeft="@dimen/dimen_10dp"
            android:padding="@dimen/dimen_10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_crm"
                android:layout_gravity="center_vertical"
                android:layout_margin="@dimen/dimen_5dp"
                android:background="@drawable/ic_general_maintenance"
                android:layout_width="@dimen/dimen_30dp"
                android:layout_height="@dimen/dimen_30dp" />

            <TextView
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/iv_crm"
                android:id="@+id/tv_crm"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/dimen_10dp"
                android:textStyle="bold"
                style="@style/style_rotobo_normal"
                android:textColor="@color/gray_color"
                android:textSize="@dimen/dimen_21dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="CRM" />


            <ImageView
                android:layout_alignParentEnd="true"
                android:layout_margin="@dimen/dimen_5dp"
                android:backgroundTint="@color/white_color"
                android:background="@drawable/ic_baseline_chevron_right_24"
                android:layout_width="@dimen/dimen_30dp"
                android:layout_height="@dimen/dimen_30dp" />

        </RelativeLayout>
        <View
            android:background="@color/gray_color"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1dp"/>


        <RelativeLayout
            android:layout_marginRight="@dimen/dimen_10dp"
            android:layout_marginLeft="@dimen/dimen_10dp"
            android:padding="@dimen/dimen_10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_wms"
                android:layout_gravity="center_vertical"
                android:layout_margin="@dimen/dimen_5dp"
                android:background="@drawable/ic_general_maintenance"
                android:layout_width="@dimen/dimen_30dp"
                android:layout_height="@dimen/dimen_30dp" />

            <TextView
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/iv_wms"
                android:id="@+id/tv_wms"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/dimen_10dp"
                android:textStyle="bold"
                style="@style/style_rotobo_normal"
                android:textColor="@color/gray_color"
                android:textSize="@dimen/dimen_21dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="WMS" />


            <ImageView
                android:layout_alignParentEnd="true"
                android:layout_margin="@dimen/dimen_5dp"
                android:backgroundTint="@color/white_color"
                android:background="@drawable/ic_baseline_chevron_right_24"
                android:layout_width="@dimen/dimen_30dp"
                android:layout_height="@dimen/dimen_30dp" />

        </RelativeLayout>
        <View
            android:background="@color/gray_color"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1dp"/>

        <RelativeLayout
            android:layout_marginRight="@dimen/dimen_10dp"
            android:layout_marginLeft="@dimen/dimen_10dp"
            android:padding="@dimen/dimen_10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_reminders"
                android:layout_gravity="center_vertical"
                android:layout_margin="@dimen/dimen_5dp"
                android:background="@drawable/ic_general_maintenance"
                android:layout_width="@dimen/dimen_30dp"
                android:layout_height="@dimen/dimen_30dp" />

            <TextView
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/iv_reminders"
                android:id="@+id/tv_reminders"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/dimen_10dp"
                android:textStyle="bold"
                style="@style/style_rotobo_normal"
                android:textColor="@color/gray_color"
                android:textSize="@dimen/dimen_21dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Reminders" />


            <ImageView
                android:layout_alignParentEnd="true"
                android:layout_margin="@dimen/dimen_5dp"
                android:backgroundTint="@color/white_color"
                android:background="@drawable/ic_baseline_chevron_right_24"
                android:layout_width="@dimen/dimen_30dp"
                android:layout_height="@dimen/dimen_30dp" />

        </RelativeLayout>
        <View
            android:background="@color/gray_color"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1dp"/>


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>