                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/dimen_15dp"
                        android:layout_height="@dimen/dimen_15dp"
                        android:layout_marginTop="@dimen/dimen_10dp"
                        android:layout_marginRight="@dimen/dimen_2dp"
                        android:background="@drawable/mobile_app_aws_bg"
                        android:padding="@dimen/dimen_2dp" />

                    <TextView
                        style="@style/style_rotobo_medium_gray"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/dimen_5dp"
                        android:text="Mobile App" />

                    <ImageView
                        android:layout_width="@dimen/dimen_15dp"
                        android:layout_height="@dimen/dimen_15dp"
                        android:layout_marginTop="@dimen/dimen_10dp"
                        android:layout_marginRight="@dimen/dimen_2dp"
                        android:background="@drawable/justdial_aws_bg"
                        android:padding="@dimen/dimen_2dp" />

                    <TextView
                        style="@style/style_rotobo_medium_gray"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/dimen_5dp"
                        android:text="Justdial" />


                    <ImageView
                        android:layout_width="@dimen/dimen_15dp"
                        android:layout_height="@dimen/dimen_15dp"
                        android:layout_marginTop="@dimen/dimen_10dp"
                        android:layout_marginRight="@dimen/dimen_2dp"
                        android:background="@drawable/walkin_aws_bg"
                        android:padding="@dimen/dimen_2dp" />

                    <TextView
                        style="@style/style_rotobo_medium_gray"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/dimen_5dp"
                        android:text="Walk-In" />
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="750dp"
                android:layout_below="@id/l_chart"
                android:layout_marginVertical="@dimen/dimen_50dp"
                android:orientation="vertical">


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="350dp"
                    android:background="@color/card_color"
                    android:orientation="vertical">

                    <TextView
                        android:layout_marginHorizontal="@dimen/dimen_10dp"
                        android:layout_marginTop="@dimen/dimen_10dp"
                        style="@style/style_rotobo_medium_gray"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="Representative"
                        android:textSize="@dimen/dimen_21dp"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_marginHorizontal="@dimen/dimen_10dp"
                        android:layout_marginTop="@dimen/dimen_15dp"
                        android:id="@+id/tv_viewMore"
                        style="@style/style_rotobo_medium_gray"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:text="View More" />

                    <View
                        android:id="@+id/viewName1"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dimen_1dp"
                        android:layout_below="@id/tv_viewMore"
                        android:layout_marginTop="@dimen/dimen_15dp"
                        android:background="@color/gray_color" />

                    <RelativeLayout
                        android:layout_below="@id/viewName1"
                        android:id="@+id/name_lay1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/dimen_21dp"
                        android:layout_marginTop="@dimen/dimen_5dp">
                        <TextView
                            android:id="@+id/tv_representative1"
                            style="@style/style_rotobo_medium_gray"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dimen_10dp"
                            android:text="Varshita" />

                        <ProgressBar
                            android:id="@+id/prgressBarRepersentative1"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tv_representative1"
                            android:progress="60"
                            android:progressBackgroundTint="#858484"
                            android:secondaryProgressTint="@color/blue_color" />

                        <TextView
                            style="@style/style_rotobo_medium_gray"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_marginTop="@dimen/dimen_10dp"
                            android:text="464 Conversions" />

                    </RelativeLayout>

                    <RelativeLayout
