                            android:backgroundTint="#0B6AB5" />

                        <TextView
                            android:id="@+id/tvMissedLead"
                            style="@style/style_rotobo_medium_gray"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@+id/iv_leadMissed"
                            android:text="Missed"
                            android:textSize="@dimen/size_11sp" />

                        <TextView
                            style="@style/style_rotobo_medium_gray"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tvMissedLead"
                            android:layout_toRightOf="@+id/iv_leadMissed"
                            android:text="5521"
                            android:textColor="@color/white_color"
                            android:textSize="@dimen/size_11sp"
                            android:textStyle="bold" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
