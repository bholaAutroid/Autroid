                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tv_representative2"
                            android:progress="60"
                            android:progressBackgroundTint="#858484"
                            android:secondaryProgressTint="@color/blue_color" />

                        <TextView
                            style="@style/style_rotobo_medium_gray"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_marginTop="@dimen/dimen_10dp"
                            android:text="464 Conversions" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_below="@id/name_lay2"
                        android:id="@+id/name_lay3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/dimen_21dp"
                        android:layout_marginTop="@dimen/dimen_5dp">
                        <TextView
                            android:id="@+id/tv_representative3"
                            style="@style/style_rotobo_medium_gray"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dimen_10dp"
                            android:text="Varha" />

                        <ProgressBar
                            android:id="@+id/prgressBarRepersentative3"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tv_representative3"
                            android:progress="60"
                            android:progressBackgroundTint="#858484"
                            android:secondaryProgressTint="@color/blue_color" />

                        <TextView
                            style="@style/style_rotobo_medium_gray"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_marginTop="@dimen/dimen_10dp"
                            android:text="464 Conversions" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_below="@id/name_lay3"
                        android:id="@+id/name_lay4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/dimen_21dp"
                        android:layout_marginTop="@dimen/dimen_5dp">
                        <TextView
                            android:id="@+id/tv_representative4"
                            style="@style/style_rotobo_medium_gray"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dimen_10dp"
                            android:text="Manish" />

                        <ProgressBar
                            android:id="@+id/prgressBarRepersentative4"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tv_representative4"
                            android:progress="60"
                            android:progressBackgroundTint="#858484"
                            android:secondaryProgressTint="@color/blue_color" />

                        <TextView
                            style="@style/style_rotobo_medium_gray"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_marginTop="@dimen/dimen_10dp"
