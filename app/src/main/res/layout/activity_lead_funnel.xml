                            style="@style/style_rotobo_medium_gray"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@+id/iv_leadLost"
                            android:text="Lost Leads"
                            android:textSize="@dimen/size_11sp" />

                        <TextView
                            style="@style/style_rotobo_medium_gray"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tvLostLead"
                            android:layout_toRightOf="@+id/iv_leadLost"
                            android:text="3234"
                            android:textColor="@color/white_color"
                            android:textSize="@dimen/size_11sp"
                            android:textStyle="bold" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/bg_aws_items"
                        android:padding="@dimen/dimen_10dp">

                        <ImageView
                            android:id="@+id/iv_leadConfirm"
                            android:layout_width="@dimen/dimen_30dp"
                            android:layout_height="@dimen/dimen_30dp"
                            android:background="@drawable/ic_leads"
                            android:backgroundTint="#0B6AB5" />

                        <TextView
                            android:id="@+id/tvConfirmLead"
                            style="@style/style_rotobo_medium_gray"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@+id/iv_leadConfirm"
                            android:text="Confirmed"
                            android:textSize="@dimen/size_11sp" />

                        <TextView
                            style="@style/style_rotobo_medium_gray"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tvConfirmLead"
                            android:layout_toRightOf="@+id/iv_leadConfirm"
                            android:text="324"
                            android:textColor="@color/white_color"
                            android:textSize="@dimen/size_11sp"
                            android:textStyle="bold" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/dimen_10dp"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_20dp"
                        android:background="@drawable/bg_aws_items"
                        android:padding="@dimen/dimen_10dp">

                        <ImageView
